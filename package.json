{
  "private": true,
  "workspaces": [
    "example",
    "use-upload"
  ],
  "scripts": {
    "build": "yarn build:hook && yarn build:example",
    "build:example": "yarn workspace example package",
    "build:hook": "yarn workspace use-upload build",
    "bump": "yarn workspace use-upload bump",
    "develop": "yarn watch",
    "watch": "concurrently -n Hook,Example \"yarn watch:hook\" \"yarn watch:example\"",
    "watch:example": "yarn workspace example develop",
    "watch:hook": "yarn workspace use-upload watch"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.2"
  },
  "license": "MIT"
}
